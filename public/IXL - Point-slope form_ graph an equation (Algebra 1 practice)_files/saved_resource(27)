YUI.add("practice-InlineMaxWidth",function(a){var t=a.namespace("IXL");function e(t){var e,i=[],n=new a.NodeList;return!!t&&(e=t.bInvokeOnResize,i=t.aSiblingSelector,this.nContainer=a.one(t.containerSelector),this.nTarget=a.one(t.targetSelector),!(!this.nContainer||!this.nTarget)&&(i.forEach(function(t){a.all(t).each(function(t){n.push(t)})}),this.nlSibling=n,this.fCalculateApplyMaxWidth(),void(e&&a.on("windowresize",this.fCalculateApplyMaxWidth,null,this))))}e.prototype={iTolerance:5,nContainer:null,nTarget:null,nlSibling:null,fExtractPxValue:function(t){var e=t.match(/[0-9]+px/g);return null!==e?parseInt(e[0].split("px")[0],10):0},fCalculateAvailableWidth:function(t){var e=t.get("region").width,i=[t.getStyle("paddingLeft"),t.getStyle("paddingRight"),t.getStyle("borderLeft"),t.getStyle("borderRight")],n=this;return i.forEach(function(t){e-=n.fExtractPxValue(t)}),e},fCalculateOccupiedWidth:function(t){var e=t.get("region").width,i=[t.getStyle("marginLeft"),t.getStyle("marginRight")],n=this;return i.forEach(function(t){e+=n.fExtractPxValue(t)}),e},fCalculateApplyMaxWidth:function(){var e=this.fCalculateAvailableWidth(this.nContainer);this.nlSibling.each(function(t){e-=this.fCalculateOccupiedWidth(t)},this),e-=this.iTolerance,this.nTarget.setStyle("maxWidth",e)}},t.InlineMaxWidth=e},"@VERSION@",{requires:["event"]});
