YUI.add("q-ExpressionUtils",function(e){e.namespace("IXL").ExpressionUtils={evalAsNumber:function e(a,r){switch(a.objectType){case"Variable":return r[a.name];case"Leaf":return a.value;case"Cos":return Math.cos(e(a.operands[0],r));case"Sin":return Math.sin(e(a.operands[0],r));case"Tan":return Math.tan(e(a.operands[0],r));case"Csc":return 1/Math.sin(e(a.operands[0],r));case"Sec":return 1/Math.cos(e(a.operands[0],r));case"Cot":return 1/Math.tan(e(a.operands[0],r));case"Plus":return e(a.operands[0],r)+e(a.operands[1],r);case"Minus":return e(a.operands[0],r)-e(a.operands[1],r);case"Times":return e(a.operands[0],r)*e(a.operands[1],r);case"Divide":return e(a.operands[0],r)/e(a.operands[1],r);case"Exponent":return Math.pow(e(a.operands[0],r),e(a.operands[1],r));case"AbsoluteValue":return Math.abs(e(a.operands[0],r));case"Negative":return-1*e(a.operands[0],r);case"Root":var s=e(a.operands[0],r),n=e(a.operands[1],r);return s<0&&n%2==1?-Math.pow(-s,1/n):Math.pow(s,1/n);case"Log":return Math.log(e(a.operands[1],r))/Math.log(e(a.operands[0],r));case"Parentheses":return e(a.operands[0],r);default:var t=a.get&&a.get("objectType")?'Cannot evaluate components. Type was "'+a.get("objectType")+'".':'Evaluation for "'+a.objectType+'" is not implemented.';throw new Error(t)}}}},"@VERSION@",{requires:["base","node","array-extras"]});
