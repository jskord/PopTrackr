YUI.add("yui-gallery-focus-position",function(o){o.namespace("IXL");var n=o.Node.prototype.focus;o.Node.prototype.focus=function(e){if("end"===e)e=this.get("value").length;else if(!o.Lang.isNumber(e))return void n.apply(this);var t,a=this._node;a.setSelectionRange?(a.focus(),a.setSelectionRange(e,e)):a.createTextRange&&((t=a.createTextRange()).collapse(!0),t.moveEnd("character",e),t.moveStart("character",e),t.select())}},"@VERSION@",{requires:[]});
